<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".HomeClient">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_65sdp"
            android:background="@drawable/back_home"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/back" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/notification"
            android:layout_marginTop="@dimen/_13sdp"
            android:layout_marginStart="@dimen/_20sdp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/navcation" />


        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="@dimen/_45sdp"
            app:layout_constraintTop_toBottomOf="@id/back"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:src="@drawable/glass"
            android:layout_height="@dimen/_45sdp"
            android:id="@+id/imageuser" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@id/imageuser"
            app:layout_constraintTop_toTopOf="@id/imageuser"
            android:text="@string/user_name"
            android:layout_marginStart="@dimen/_15sdp"
            android:textColor="#182A41"
            android:textSize="@dimen/_13sdp"
            android:id="@+id/username" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@id/imageuser"
            app:layout_constraintTop_toBottomOf="@id/username"
            app:layout_constraintBottom_toBottomOf="@id/imageuser"
            android:text=" Title Job"
            android:layout_marginStart="@dimen/_15sdp"
            android:textColor="#182A41"
            android:layout_marginTop="@dimen/_10sdp"
            android:textSize="@dimen/_13sdp"
            android:id="@+id/namejob" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@id/imageuser"
            app:layout_constraintTop_toBottomOf="@id/namejob"

            android:text=" number"
            android:layout_marginStart="@dimen/_15sdp"
            android:textColor="#182A41"
            android:layout_marginTop="@dimen/_10sdp"
            android:textSize="@dimen/_13sdp"
            android:id="@+id/numberphone" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@id/imageuser"
            app:layout_constraintTop_toBottomOf="@id/numberphone"

            android:text=" number"
            android:layout_marginStart="@dimen/_15sdp"
            android:textColor="#182A41"
            android:layout_marginTop="@dimen/_10sdp"
            android:textSize="@dimen/_13sdp"
            android:id="@+id/Email" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@id/imageuser"
            app:layout_constraintTop_toBottomOf="@id/Email"
            android:text=" number"
            android:layout_marginStart="@dimen/_15sdp"
            android:textColor="#182A41"
            android:layout_marginTop="@dimen/_10sdp"
            android:textSize="@dimen/_13sdp"
            android:id="@+id/Addriss" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#BA8A13"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/Addriss"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:id="@+id/view"
            android:layout_marginEnd="@dimen/_15sdp" />


        <ImageView
            android:id="@+id/note"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:src="@drawable/law"
            app:layout_constraintEnd_toStartOf="@+id/out"
            app:layout_constraintStart_toEndOf="@id/changepassword"
            app:layout_constraintTop_toBottomOf="@id/view" />

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_20sdp"
            android:id="@+id/changepassword"
            android:src="@drawable/secure" />

        <ImageView

            android:id="@+id/out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:src="@drawable/out"
            android:layout_marginEnd="@dimen/_20sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view" />

        <TextView

            android:id="@+id/customers"
            android:layout_width="0dp"
            android:layout_height="@dimen/_90sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_13sdp"
            android:background="@drawable/item_blu"
            android:drawableTop="@drawable/law"
            android:gravity="center"
            android:padding="@dimen/_11sdp"
            android:paddingBottom="@dimen/_15sdp"
            android:text="القضايا"
            android:textColor="@color/white"
            android:textSize="@dimen/_13sdp"
            app:layout_constraintEnd_toStartOf="@+id/users3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/note" />

        <TextView
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_width="0dp"
            android:layout_height="@dimen/_90sdp"
            android:id="@+id/users3"
            app:layout_constraintStart_toEndOf="@id/customers"
            android:text="الاستشارات"
            android:paddingBottom="@dimen/_15sdp"
            android:gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/note"
            android:layout_marginTop="@dimen/_13sdp"
            android:layout_marginEnd="@dimen/_15sdp"
            android:padding="@dimen/_15sdp"
            android:textSize="@dimen/_15sdp"
            android:textColor="@color/white"
            android:background="@drawable/item_qdaya"
            android:drawableTop="@drawable/legal" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/customers"
            android:text="طلب استشاره جديده"
            android:layout_marginStart="@dimen/_15sdp"
            android:textColor="#BA8A13"
            android:textSize="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:id="@+id/newRequste" />

        <Spinner
            android:id="@+id/spiner"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginTop="@dimen/_15sdp"
            android:layout_marginEnd="@dimen/_20sdp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/newRequste" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/spiner"
            android:layout_marginTop="@dimen/_20sdp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:id="@+id/text" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/Add"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16sdp"
            android:layout_marginStart="@dimen/_16sdp"
            android:background="@drawable/item_btn_register"
            android:text="ارسال"
            android:textAllCaps="false"
            android:textColor="#182A41"
            android:textSize="@dimen/_15sdp"
            android:textStyle="bold"
            app:backgroundTint="@null"
            app:backgroundTintMode="add"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/text" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>